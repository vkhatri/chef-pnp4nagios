#
# This file is managed by Chef.
# Do NOT modify this file directly.
#
# definitions for PNP in nagios.cfg (icinga.cfg)
# please make sure that you don't have duplicate entries
#
# Synchronous mode
#
process_performance_data=1

enable_environment_macros=1	# available since Nagios 3.x

service_perfdata_command=process-service-perfdata

host_perfdata_command=process-host-perfdata	# NOT advisable prior to Nagios 3.0

#
# Bulk / NPCD mode
#

process_performance_data=1

# *** the template definition differs from the one in the original nagios.cfg
#
service_perfdata_file=<%= @service_data_dir -%>
service_perfdata_file_template=<%= @service_data_file_template -%>
service_perfdata_file_mode=a
service_perfdata_file_processing_interval=<%= @service_data_processing_interval -%>
service_perfdata_file_processing_command=process-service-perfdata-file

# *** the template definition differs from the one in the original nagios.cfg
#
host_perfdata_file=<%= @host_data_dir -%>
host_perfdata_file_template=<%= @host_data_file_template -%>
host_perfdata_file_mode=a
host_perfdata_file_processing_interval=<%= @host_data_processing_interval -%>
host_perfdata_file_processing_command=process-host-perfdata-file

#
# Module mode
#
event_broker_options=-1 # minimum 4+8=12
process_performance_data=1
broker_module=<%= @install_dir -%>/lib/npcdmod.o
config_file=<%= @config_dir -%>/npcd.cfg
